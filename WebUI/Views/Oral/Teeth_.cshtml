                    <script id="diagnoz-diagnoz">

                        $("[data-tooltip=\"tooltip\"]").tooltip({ trigger: "hover" });
                        //$('[data-toggle="tooltip"]').tooltip();

                        if (!$(".teeth-content li:last").hasClass("active")) {
                            $(".teeth-content li:first, .teeth-content li:first a, .tab-content #daimi").addClass("active");
                            $(".teeth-content li:last, .teeth-content li:last a, .tab-content #sut").removeClass("active");
                        } else {
                            $(".teeth-content li:last, .teeth-content li:last a, .tab-content #sut").addClass("active");
                            $(".teeth-content li:first, .teeth-content li:first a, .tab-content #daimi").removeClass("active");
                        }

                        if ($(window).width() < 768) {
                            $(".teeth-content #daimi").css("margin-left", "-12px");
                        } else {
                            $(".teeth-content #daimi").removeAttr("style");
                        }

                        var tedavi_kod = [];
                        var dis_no = [];
                        var kok_sayisi = [];

                        for (var i = 0; i < tedavi_kod.length; ++i) {

                            // Z-index Atama
                            var zindex_bulma = (function() {
                                var index_highest = 1;
                                $("a.d-" + dis_no[i] + " span").each(function() {
                                    var index_current = parseInt($(this).css("zIndex"), 10);
                                    if (index_current > index_highest) {
                                        index_highest = index_current;
                                    }
                                });
                                return function returnFunction() {
                                    return index_highest;
                                };
                            })();

                            switch (tedavi_kod[i]) {

                                // Diş Çekimleri
                                case "05-001-001":
                                case "05-002-001":
                                case "05-003-001":
                                case "05-004-001":
                                    $("a.d-" + dis_no[i] + " span").css("display", "none");
                                    $("a.d-" + dis_no[i]).attr("data-cekim", "1");
                                    break;

                                // Reimplantasyon
                                case "05-030-001":
                                case "05-031-001":
                                    $("a.d-" + dis_no[i]).append("<span class=\"i-" + tedavi_kod[i] + "-" + dis_no[i] + "\" style=\"z-index:" + zindex_bulma() + 1 + "\"></span>");
                                    break;

                                // İmplant
                                case "05-035-001":
                                case "05-036-001":
                                    $("a.d-" + dis_no[i] + " span").css("display", "none");
                                    $("a.d-" + dis_no[i]).append("<span class=\"i-" + tedavi_kod[i] + "-" + dis_no[i] + "\" style=\"z-index:" + zindex_bulma() + 1 + "\"></span>");
                                    $("a.d-" + dis_no[i]).attr("data-implant", "1");
                                    break;

                                // İmplant Çıkartması
                                case "05-052-001":
                                    $("a.d-" + dis_no[i]).find("span").each(function(index) {
                                        if ($("a.d-" + dis_no[i] + " span").hasClass("i-05-035-001-" + dis_no[i])) {
                                            $("a.d-" + dis_no[i] + " span.i-05-035-001-" + dis_no[i]).css("display", "none");
                                        }
                                    });
                                    $("a.d-" + dis_no[i]).append("<span class=\"i-" + tedavi_kod[i] + "-" + dis_no[i] + "\" style=\"z-index:" + zindex_bulma() + 1 + "\"></span>");
                                    break;

                                // Sert Doku Greftleme
                                case "05-019-001":
                                    $("a.d-" + dis_no[i]).append("<span class=\"i-" + tedavi_kod[i] + "-" + dis_no[i] + "-1\" style=\"z-index:" + zindex_bulma() + 1 + "\"></span>");
                                    break;

                                // Sinüs Lifting
                                case "05-021-001":
                                    $(".or-d-" + dis_no[i]).css("display", "none");
                                    $("a.d-" + dis_no[i]).append("<span class=\"i-" + tedavi_kod[i] + "-" + dis_no[i] + " style=\"z-index:" + zindex_bulma() + 1 + "\"></span>");
                                    break;

                                // Kötü Kanal Tedavisi
                                // Tek Kök Kanal Tedavisi, Tek Kök Rezeksiyon, Tek Kök Periapikal Lezyon
                                // İki Kök Kanal Tedavisi, İki Kök Rezeksiyon, İki Kök Periapikal Lezyon
                                // Üç Kök Kanal Tedavisi, Üç Kök Rezeksiyon, Üç Kök Periapikal Lezyon
                                case "02-036-001":
                                case "02-013-001":
                                case "05-005-001":
                                case "02-017-001":
                                case "02-014-001":
                                case "05-006-001":
                                case "02-018-001":
                                case "02-015-001":
                                case "05-007-001":
                                case "02-019-001":
                                    var parcala = kok_sayisi[i].split(",");
                                    for (var n = 0; n < parcala.length; n++) {
                                        var gecici_class = $("a.d-" + dis_no[i] + " span").first().attr("class");
                                        var sonuc = gecici_class.substring(5, 7);
                                        var deger = sonuc.replace(sonuc, dis_no[i]);
                                        $("a.d-" + deger).append("<span class=\"i-" + tedavi_kod[i] + "-" + sonuc + "-" + parcala[n] + "\" style=\"z-index:" + zindex_bulma() + 1 + "\"></span>");
                                    }
                                    break;

                                // Düz Ark Teli, Braket Tatbiki
                                case "07-034-001":
                                case "07-038-001":

                                    if (dis_no[i] == "18-17-16-15-14-13-12-11-21-22-23-24-25-26-27-28") {
                                        for (var y = 1; y <= 8; ++y) {
                                            $("a.d-1" + y).append("<span class=\"i-" + tedavi_kod[i] + "-1" + y + "\" style=\"z-index:" + zindex_bulma() + 1 + "\"></span>");
                                        }
                                        for (var z = 1; z <= 8; ++z) {
                                            $("a.d-2" + z).append("<span class=\"i-" + tedavi_kod[i] + "-2" + z + "\" style=\"z-index:" + zindex_bulma() + 1 + "\"></span>");
                                        }

                                    } else if (dis_no[i] == "48-47-46-45-44-43-42-41-31-32-33-34-35-36-37-38") {
                                        for (var x = 1; x <= 8; ++x) {
                                            $("a.d-4" + x).append("<span class=\"i-" + tedavi_kod[i] + "-4" + x + "\" style=\"z-index:" + zindex_bulma() + 1 + "\"></span>");
                                        }
                                        for (var a = 1; a <= 8; ++a) {
                                            $("a.d-3" + a).append("<span class=\"i-" + tedavi_kod[i] + "-3" + a + "\" style=\"z-index:" + zindex_bulma() + 1 + "\"></span>");
                                        }

                                    } else if (dis_no[i] == "55-54-53-52-51-61-62-63-64-65") {
                                        for (var x = 1; x <= 8; ++x) {
                                            $("a.d-4" + x).append("<span class=\"i-" + tedavi_kod[i] + "-4" + x + "\" style=\"z-index:" + zindex_bulma() + 1 + "\"></span>");
                                        }
                                        for (var a = 1; a <= 8; ++a) {
                                            $("a.d-3" + a).append("<span class=\"i-" + tedavi_kod[i] + "-3" + a + "\" style=\"z-index:" + zindex_bulma() + 1 + "\"></span>");
                                        }

                                    } else {
                                        for (var x = 1; x <= 8; ++x) {
                                            $("a.d-4" + x).append("<span class=\"i-" + tedavi_kod[i] + "-4" + x + "\" style=\"z-index:" + zindex_bulma() + 1 + "\"></span>");
                                        }
                                        for (var a = 1; a <= 8; ++a) {
                                            $("a.d-3" + a).append("<span class=\"i-" + tedavi_kod[i] + "-3" + a + "\" style=\"z-index:" + zindex_bulma() + 1 + "\"></span>");
                                        }
                                    }
                                    break;

                                // Bant Tatbiki
                                case "07-037-001":
                                    $("a.d-" + dis_no[i]).append("<span class=\"i-" + tedavi_kod[i] + "-" + dis_no[i] + "\" style=\"z-index:" + zindex_bulma() + 1 + "\"></span>");
                                    break;

                                // Sabit Pekiştirme Aygıtı Retainer
                                case "07-023-001":
                                    if (dis_no[i] == "13-12-11-21-22-23") {
                                        for (var y = 1; y <= 3; ++y) {
                                            $("a.d-1" + y).append("<span class=\"i-" + tedavi_kod[i] + "-1" + y + "\" style=\"z-index:" + zindex_bulma() + 1 + "\"></span>");
                                        }
                                        for (var z = 1; z <= 3; ++z) {
                                            $("a.d-2" + z).append("<span class=\"i-" + tedavi_kod[i] + "-2" + z + "\" style=\"z-index:" + zindex_bulma() + 1 + "\"></span>");
                                        }

                                    } else if (dis_no[i] == "43-42-41-31-32-33") {
                                        for (var x = 1; x <= 3; ++x) {
                                            $("a.d-4" + x).append("<span class=\"i-" + tedavi_kod[i] + "-4" + x + "\" style=\"z-index:" + zindex_bulma() + 1 + "\"></span>");
                                        }
                                        for (var a = 1; a <= 3; ++a) {
                                            $("a.d-3" + a).append("<span class=\"i-" + tedavi_kod[i] + "-3" + a + "\" style=\"z-index:" + zindex_bulma() + 1 + "\"></span>");
                                        }

                                    }
                                    break;

                                // Amputasyon, Dolgu
                                case "03-008-001":
                                case "03-008-002":
                                case "03-010-001":
                                    var gecici_class = $("a.d-" + dis_no[i] + " span").first().attr("class");
                                    var sonuc = gecici_class.substring(5, 7);
                                    var deger = sonuc.replace(sonuc, dis_no[i]);
                                    $("a.d-" + deger).append("<span class=\"i-" + tedavi_kod[i] + "-" + sonuc + "\" style=\"z-index:" + zindex_bulma() + 1 + "\"></span>");
                                    break;

                                // Yer Tutucu Sabit ve Hareketli
                                case "03-004-001":
                                case "03-005-001":
                                    var parcala = dis_no[i].split("-");
                                    for (var n = 0; n < parcala.length; ++n) {
                                        var gecici_class = $("a.d-" + parcala[n] + " span").first().attr("class");
                                        $("a.d-" + parcala[n]).append("<span class=\"i-" + tedavi_kod[i] + "-" + gecici_class.substring(5, 7) + "\" style=\"z-index:" + zindex_bulma() + 1 + "\"></span>");
                                    }
                                    break;

                                // Subgingival Küretaj (Tek Diş), Gingivitis, Periodontitis
                                case "06-003-001":
                                case "06-019-001":
                                case "06-020-001":
                                    $("a.d-" + dis_no[i]).append("<span class=\"i-" + tedavi_kod[i] + "-" + dis_no[i] + "\" style=\"z-index:" + zindex_bulma() + 1 + "\"></span>");
                                    break;

                                // Tam Protezler (Akrilik - Metal)
                                case "04-001-001":
